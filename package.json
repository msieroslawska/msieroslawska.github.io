{
  "name": "msieroslawska.github.io",
  "version": "2.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "deploy": "next build && next export -o docs && touch docs/.nojekyll && git add docs && git commit -m \"chore: regen site\" && git push origin master",
    "type-check": "tsc",
    "style": "stylelint \"**/*.scss\" --color",
    "style:fix": "stylelint \"**/*.scss\" --color --fix",
    "eslint": "eslint --ext .ts,.tsx ./",
    "eslint:fix": "eslint --fix --ext .ts,.tsx ./"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run style && npm run eslint"
    }
  },
  "dependencies": {
    "gray-matter": "4.0.2",
    "next": "latest",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "remark-highlight.js": "6.0.0",
    "remark-html": "11.0.2",
    "remark-parse": "8.0.2",
    "sass": "1.26.10",
    "unified": "9.0.0"
  },
  "devDependencies": {
    "@types/node": "14.0.14",
    "@types/react": "16.9.41",
    "@types/react-dom": "16.9.8",
    "@types/uuid": "8.0.0",
    "@typescript-eslint/eslint-plugin": "3.5.0",
    "@typescript-eslint/parser": "3.5.0",
    "eslint": "7.4.0",
    "eslint-config-airbnb-typescript": "8.0.2",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.3",
    "eslint-plugin-react-hooks": "4.0.5",
    "husky": "4.2.5",
    "prettier": "2.0.5",
    "rimraf": "3.0.2",
    "stylelint": "13.6.1",
    "stylelint-config-sass-guidelines": "7.0.0",
    "stylelint-config-standard": "20.0.0",
    "typescript": "3.9.6"
  },
  "license": "ISC"
}
