{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "jsx": "preserve",
    "lib": [
      "dom"
    ],
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "include": [
    "**/*",
    "./src/declaration.d.ts",
  ],
  "exclude": [
    "node_modules",
    "public",
    ".cache"
  ]
}